  "license": "MIT",
  "keywords": [
    "modx", "migrate", "seed"
  ],
  "authors": [
    {
      "name": "Joshua Gulledge",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "LCI\\Blend\\": "src/"
    }
  },
  "require": {
    "php" : ">=7.0.0",
    "symfony/console": ">=3.4",
    "lci/console": ">=1.1.4"
  },
  "bin": [
    "bin/blend"
  ],
  "require-dev": {
    "phpunit/phpunit": "^6"
  }
}
