{
  "name": "lci/blend",
  "description": "This project aims to import/export resources and elements from MODX to another MODX install to allow easier dev to prod git workflows.",
  "license": "MIT",
  "keywords": [
    "modx", "migrate", "seed"
  ],
  "authors": [
    {
      "name": "Joshua Gulledge",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "LCI\\Blend\\": "src/"
    }
  },
  "require": {
    "php" : ">=5.6.0",
    "symfony/console": ">=3.4",
    "guzzlehttp/guzzle": "^6.3",
    "composer/composer": ">=1.6",
    "lci/console": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/LippertComponents/Console"
    }
  ],

  "minimum-stability": "dev",

  "bin": [
    "bin/blend"
  ],
  "require-dev": {
    "phpunit/phpunit": "^6"
  }
}
